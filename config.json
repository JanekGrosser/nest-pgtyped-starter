{
  "transforms": [
    {
      "mode": "sql",
      "include": "**/*.sql",
      "emitTemplate": "{{dir}}/{{name}}.queries.ts"
    },
    {
      "mode": "ts",
      "include": "**/*.ts",
      "emitTemplate": "{{dir}}/{{name}}.types.ts"
    }
  ],
  "srcDir": "./src/",
  "dbUrl": "postgresql://postgres:root@127.0.0.1:5482/pg_typed_starter"
}
